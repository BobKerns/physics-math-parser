{
  "name": "@rwk/physics-math-parser",
  "version": "1.0.0",
  "description": "Parser for math expressions",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "browser": "lib/umd/index.js",
  "jsdelivr": "lib/umd/index.js",
  "unpkg": "lib/umd/index.js",
  "types": "lib/esm/index.d.ts",
  "scripts": {
    "antlr4ts": "cd src && antlr4ts -visitor Math.g4 -o gen",
    "prebuild:config": "tsc --project config/tsconfig.json",
    "build:config": "",
    "prebuild:devtools": "npm run build:config",
    "build:devtools": "tsc --project devtools/tsconfig.json",
    "pretest": "tsc --project src/__tests__ && npm run build:config",
    "test": "jest",
    "prebuild": "npm run build:devtools && npm run antlr4ts",
    "build": "tsc --project src/tsconfig.json && rollup -c",
    "postbuild": "",
    "watch": "NODE_ENV=development rollup -c --watch",
    "clean": "rm -rf build lib docs",
    "prepublishOnly": "NODE_ENV=production npm run build",
    "prebuild:docs": "npm install",
    "build:docs": "GRAPHVIZ_DOT=\"$(which dot)\" typedoc --excludeNotExported --stripInternal --mode modules --out build/docs/api src/",
    "preserve": "npm run build:devtools",
    "serve": "NODE_ENV=development SERVE=true rollup -c",
    "predocs": "npm run build:config && npm run build:docs",
    "docs": "NODE_ENV=development SERVE=true SERVE_DOC=true rollup -c",
    "prewatch": "npm run build:devtools"
  },
  "bin": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/bobkerns/physics-math-parser.git"
  },
  "keywords": [
    "physics",
    "math",
    "parser"
  ],
  "author": "Bob Kerns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BobKerns/physics-math-parser/issues"
  },
  "homepage": "https://github.com/BobKerns/physics-math-parser#readme",
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.3",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^24.9.1",
    "@types/ramda": "^0.26.44",
    "antlr4ts-cli": "^0.5.0-alpha.3",
    "jest": "^26.6.3",
    "jest-mock-proxy": "^3.0.0",
    "mermaid": "^8.8.4",
    "ramda": "^0.26.1",
    "rollup": "^2.35.1",
    "rollup-plugin-external-globals": "^0.5.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.21.2",
    "rollup-plugin-visualizer": "^1.1.1",
    "serve": "^11.3.2",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.17.8",
    "typedoc-plugin-external-module-name": "^3.1.0",
    "typedoc-plugin-mermaid": "^1.3.0",
    "typedoc-plugin-npm-externals": "^1.0.0",
    "typescript": "^3.9.7",
    "wcsize": "^1.0.0"
  }
}
